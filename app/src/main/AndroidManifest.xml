<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.haami.haami">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application xmlns:tools="http://schemas.android.com/tools"
        tools:replace="android:supportsRtl"
        android:name="com.haami.haami.app.AppController"
        android:allowBackup="true"
        android:icon="@drawable/home_center_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/home_center_icon"
        android:supportsRtl="false"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">

        <activity android:name="com.haami.haami.AboutActivity" />
        <activity android:name="com.haami.haami.SettingActivity" />
        <activity android:name="com.haami.haami.AgreementActivity" />
        <activity android:name="com.haami.haami.SignUpActivity" />
        <activity android:name="com.haami.haami.LoginActivity" />
        <activity android:name="com.haami.haami.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.haami.haami.MainActivity" />
    </application>

</manifest>